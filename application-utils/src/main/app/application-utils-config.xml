<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:smtp="http://www.mulesoft.org/schema/mule/smtp"
	xmlns:db="http://www.mulesoft.org/schema/mule/db"
	xmlns:secure-property-placeholder="http://www.mulesoft.org/schema/mule/secure-property-placeholder"
	xmlns:jms="http://www.mulesoft.org/schema/mule/jms" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:spring="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mulesoft.org/schema/mule/db http://www.mulesoft.org/schema/mule/db/current/mule-db.xsd
http://www.mulesoft.org/schema/mule/secure-property-placeholder http://www.mulesoft.org/schema/mule/secure-property-placeholder/current/mule-secure-property-placeholder.xsd
http://www.mulesoft.org/schema/mule/jms http://www.mulesoft.org/schema/mule/jms/current/mule-jms.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd
http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/smtp http://www.mulesoft.org/schema/mule/smtp/current/mule-smtp.xsd">

	<jms:activemq-connector name="Active_MQ"
		brokerURL="${activemq.broker.url}" validateConnections="true"
		doc:name="Active MQ" persistentDelivery="true" maxRedelivery="1"
		password="${queue.jms.password}" username="${queue.jms.username}"
		numberOfConsumers="1">
		<reconnect frequency="20000" count="200" />
	</jms:activemq-connector>
	<secure-property-placeholder:config
		name="Secure_Property_Placeholder" key="${mulevault.key}"
		location="classpath:application-utils-${mule.env}.properties,file:///opt/app/esb/app-config/application-utils-override.properties"
		doc:name="Secure Property Placeholder" ignoreResourceNotFound="true" />
	<db:oracle-config name="Oracle_Configuration" host="${oracle.host}"
		port="${oracle.port}" instance="${oracle.instance}" user="${oracle.user}"
		password="${oracle.password}" doc:name="Oracle Configuration">
		<db:pooling-profile maxPoolSize="${db.conn.maxpoolsize}"
			minPoolSize="${db.conn.minpoolsize}" />
	</db:oracle-config>
	<smtp:connector name="SMTP" contentType="text/html"
		validateConnections="true" doc:name="SMTP" />


</mule>
